{% extends 'admin/base.html.twig' %}

{% block title %}Modifier la clé{% endblock %}

{% block body %}
    {{ include("admin/header.html.twig") }} 
    {{ include("navigation/navAdmin.html.twig") }}
        <div class="row">
            <div class="col-sm-6">
                {% set car = key_car.car %}
                <h1>Modifier clé de la {{ car.name }}</h1>
            </div>
            <div class="col-sm-6">
                <a class="btn btn-primary pull-right" href="{{ path('admin_key_car_index') }}">Retour</a>
            </div>
        </div>

    {{ include('admin/key_car/_form.html.twig', {'button_label': 'Modifier'}) }}
    {{ include('admin/key_car/_delete_form.html.twig') }}
    {{ include("admin/footer.html.twig") }} 
{% endblock %}
